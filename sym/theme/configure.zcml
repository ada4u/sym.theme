<configure
    xmlns="http://namespaces.zope.org/zope"
    xmlns:five="http://namespaces.zope.org/five"
    xmlns:i18n="http://namespaces.zope.org/i18n"
    xmlns:browser="http://namespace.zope.org/browser"
    xmlns:genericsetup="http://namespace.zope.org/genericsetup"
    xmlns:zope="http://namespace.zope.org/zope"
    xmlns:plone="http://namespace.plone.org/plone"
    i18n_domain="sym.theme">

  <five:registerPackage package="." initialize=".initialize" />

  <genericsetup:registerProfile
                name="default" 
		title="sym theme"
		directory="profiles/default" 
		description="Installs the sym.theme package" 
		provides="Products.GenericSetup.interfaces.EXTENSION" />
  <plone:static
                type="theme" 
		directory="static" />
  
</configure>
